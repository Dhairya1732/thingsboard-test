              "cardCss" : "@import url('https://fonts.googleapis.com/css2?family=Orbitron:wght@500&display=swap'); /* Clean futuristic font */\r\n\r\nbody {\r\n    font-family: 'Orbitron', sans-serif;\r\n    background-color: #0d0f1a; /* Dark background for contrast */\r\n    margin: 0;\r\n    height: 100vh;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.container {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    width: 100%;\r\n    margin-top: 20%;\r\n}\r\n\r\n.card {\r\n    background-color: #1e293b; /* Simplified dark card background */\r\n    color: #ffffff;\r\n    width: 400px;\r\n    padding: 70px;\r\n    border-radius: 12px; /* Slightly rounded corners */\r\n    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2); /* Subtle shadow */\r\n    text-align: center;\r\n}\r\n\r\n.firmware-version {\r\n    font-size: 18px;\r\n    color: #fbc02d; /* Electric yellow to highlight the firmware version */\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.serial-number, .manufacturer-name {\r\n    margin: 0;\r\n    padding: 10px 0;\r\n}\r\n\r\n.serial-number {\r\n    font-size: 18px;\r\n    color: #9ea7b3; /* Soft silver color */\r\n}\r\n\r\n.manufacturer-name {\r\n    font-size: 24px;\r\n    color: #fbc02d; /* Electric yellow for emphasis */\r\n    font-weight: 600;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.meter-details p {\r\n    font-size: 16px;\r\n    color: #9ea7b3; /* Muted color for details at the bottom */\r\n    margin: 5px 0;\r\n}\r\n"
